    "#program to return nth fibonacci number\n",
    "\n",
    "def nthFibonacci(n):\n",
    "    if n == 2:\n",
    "        return 1\n",
    "    elif n == 1:\n",
    "        return 0\n",
    "    else:\n",
    "        return nthFibonacci(n-1) + nthFibonacci(n-2)"
   ]
